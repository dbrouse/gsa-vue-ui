{
  "name": "@gsa/vue-ui",
  "version": "1.0.0",
  "description": "Vue 3 UI components built for GSA by Zero41.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbrouse/gsa-vue-ui.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./tailwind-preset": "./tailwind-preset.js",
    "./styles.css": "./styles.css"
  },
  "files": [
    "dist",
    "tailwind-preset.js",
    "styles.css"
  ],
  "sideEffects": [
    "styles.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --project tsconfig.build.json",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.4 <5",
    "vue": "^3.4.0",
    "vue-icons-plus": "^0.1.8",
    "tailgrids": "^2.3.0"
  },
  "peerDependenciesMeta": {
    "tailwindcss": {
      "optional": true
    },
    "vue-icons-plus": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vue": "^3.4.0",
    "vue-icons-plus": "^0.1.8",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "license": "UNLICENSED"
}
