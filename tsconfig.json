{
  "compilerOptions": {
    "composite": false,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "preserve",
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": false,
    "noEmit": true,
    "types": ["vite/client"]
  },
  "include": ["src"]
}